@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="shortcut icon" href="/admin-side/images/pha_logo.ico" />

    <title>Login </title>

    <!-- Bootstrap -->
    <link href="~/vendor/gentelella/vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/vendor/gentelella/vendors/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <!-- NProgress -->
    <link href="~/vendor/gentelella/vendors/nprogress/nprogress.css" rel="stylesheet" />
    <!-- Animate.css -->
    <link href="~/vendor/gentelella/vendors/animate.css/animate.css" rel="stylesheet" />
    <link href="~/vendor/notifyjs/dist/styles/metro/notify-metro.css" rel="stylesheet" />

    <!-- Custom Theme Style -->
    <link href="~/admin-side/css/custom.css" rel="stylesheet" />
</head>
<body class="login">
    <div>
        <div class="login_wrapper">
            <div class="animate form login_form">
                <section class="login_content">
                    <form id="frmLogin">
                        @Html.AntiForgeryToken()
                        <div>
                            @*<img src="/admin-side/images/logoNiTi.png" alt="...">*@

                        </div>
                        <h1>BOSACO APP</h1>
                        <br />
                        <div>
                            <input type="text" class="form-control" name="userName" placeholder="User name" id="txtUserName" required="" />
                        </div>
                        <div>
                            <input type="password" class="form-control" name="password" id="txtPassword" placeholder="Password" required="" />
                        </div>
                        <div>
                            <a class="btn btn-success submit" href="#" id="btnLogin" style="color:white; text-decoration:none;">Login</a>

                        </div>
                        <br />
                        <div>
                            <div class="clearfix"></div>
                            <br />
                            <p> © 2022 Bosaco App</p>
                        </div>
                    </form>
                </section>
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/vendor/gentelella/vendors/jquery/dist/jquery.js"></script>

    <script src="~/vendor/notifyjs/dist/notify.js"></script>
    <script src="~/vendor/notifyjs/dist/styles/metro/notify-metro.js"></script>
    <script src="~/vendor/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/vendor/jquery-validation/dist/additional-methods.js"></script>
    <script src="~/vendor/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.js"></script>

    <script src="~/admin-app/shared/niti.js" asp-append-version="true"></script>
    <script src="~/admin-app/controllers/login/index.js" asp-append-version="true"></script>
    <script>
        var login = new loginController();
        login.initialize();
    </script>
</body>
</html>

