    <link href="~/cnews-side/css/selectlangue/selectlangue.css" rel="stylesheet" />

    <!-- CSS Style -->
    <link href="~/cnews-side/css/style.css" rel="stylesheet" />

    @RenderSection("Styles", required: false)
</head>

<body class="cnt-home">

    @*<!-- mobile menu -->
        @await Component.InvokeAsync("MobileMenuCShop")
        <!-- end mobile menu -->*@

    <div id="page">
        <input type="hidden" id="hidCorporationNameCN" value="@corname" />

        <input type="hidden" id="hidUserNameIdCN" value="" />
        <input type="hidden" id="hidUserFullNameCN" value="" />
        <input type="hidden" id="hidUserImageCN" value="" />
        <input type="hidden" id="hidAvatarUserIdCN" value="" />

        <!-- Header -->
        <Partial name="_Header.cshtml" />
        <!-- End Header -->
        @*
            <nav>
                <div class="container">
                    <div class="row">
