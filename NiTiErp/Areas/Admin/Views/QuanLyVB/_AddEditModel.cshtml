<div id="modal-add-edit-QuanLyVanBan" class="bootbox modal fade modal-darkorange in" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog  modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="bootbox-close-button close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Thêm mới Quản lý văn bản </h4>
            </div>
            <div class="modal-body">
                <div id="horizontal-form">
                    <form class="form-horizontal form-label-left input_mask" role="form" id="frmMainQuanLyVanBan">
                        <input type="hidden" id="hidQuanLyVanBanId" value="" />

                        <input type="hidden" id="hidInsertQuanLyVanBanId" value="" />

                        <div class="form-group">
                            <div class="col-sm-6">
                                <label for="lbKhuVucQLVB">Khu vực QLVB <a style="color:red">(*)</a></label>                               
                                <select id="ddlKhuVucQLVB" class="form-control"></select>                               
                            </div>                            
                        </div>
                        <div class="form-group">
                            <div class="col-sm-6">
                                <label for="lbNamHinhThanh">Năm hình thành <a style="color:red">(*)</a></label>
                                <input type="number" name="txtNamHinhThanh" class="form-control has-feedback-right" id="txtNamHinhThanh" placeholder="Năm hình thành">
                            </div>
                            <div class="col-sm-6">
                                <label for="lbSoVaKyHieu">Số và ký hiệu <a style="color:red">(*)</a></label>
                                <input type="text" name="txtSoVaKyHieu" class="form-control has-feedback-right" id="txtSoVaKyHieu" placeholder="Số và ký hiệu">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-6">
                                <label for="lbTieuDe">Tiêu đề <a style="color:red">(*)</a></label>
                                <input type="text" name="txtTieuDe" class="form-control has-feedback-right" id="txtTieuDe" placeholder="Tiểu đề">
                            </div>
                            <div class="col-sm-6">
                                <label for="lbThoiHanBaoQuan">Thời hạn bảo quản <a style="color:red">(*)</a></label>
                                <input type="number" name="txtThoiHanBaoQuan" class="form-control has-feedback-right" id="txtThoiHanBaoQuan" placeholder="Thời hạn bảo quản">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <button type="button" id="btnSaveQuanLyVanBan" class="btn btn-primary">Lưu </button>
                                <button type="button" id="btnCancelQuanLyVanBan" data-dismiss="modal" class="btn btn-default">Đóng</button>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="ln_solid"></div>

            </div>
        </div>
    </div>
</div>