<div id="modal-add-edit-EditDiaDiemCongTacAddHinh" class="bootbox modal fade modal-darkorange in" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" style="overflow-y: auto; max-height:85%;  margin-top: 50px; margin-bottom:50px; ">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="bootbox-close-button close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Upload hình - Video </h4>
            </div>
            <div class="modal-body">
                <div id="horizontal-form">
                    <form class="form-horizontal form-label-left input_mask" role="form" id="frmMainDiaDiemCongTacAddHinh">

                        <div class="form-group">
                            <div class="col-sm-6">
                                <label for="lbDDCTTenFileHinh">
                                    Tên file hình - video
                                </label>
                                <input type="text" name="txtDDCTTenFileHinh" class="form-control has-feedback-right" id="txtDDCTTenFileHinh" placeholder="Tên file hình - video">
                            </div>
                            <div class="col-sm-6">
                                <label for="lbAddEditPathDDCTTenFileHinh">
                                    Upload file hình - video
                                </label>
                                <div id="imagelistDDCTTenFileHinh" class="col-md-12">
                                </div>
                                <div class="input-group">
                                    <input type="file" id="fileDDCTTenFileHinh" />
                                </div>
                                <progress id="fileProgress" style="display: none"></progress>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <button type="button" id="btnCancelDDCTTenFileHinh" data-dismiss="modal" class="btn btn-default">Đóng</button>
                                <button type="button" id="btnSaveDDCTTenFileHinh" class="btn btn-success">Lưu file hinh - video </button>
                            </div>
                        </div>
                        <table class="table table-striped jambo_table bulk_action" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>Tên file hình - video</th>
                                    <th>.</th>
                                </tr>
                            </thead>
                            <tbody id="table-contentDDCTTenFileHinh"></tbody>
                        </table>
                    </form>
                </div>
                <div class="ln_solid"></div>
            </div>
        </div>
    </div>
    <script type="x-tmpl-mustache" id="template-table-DDCTTenFileHinh">
        <tr>
            <td>
                <a class="btn-link btnPatchDDCTTenFileHinh" data-id="{{Id}}" data-toggle="tooltip">
                     {{TenFileHinh}}
                </a>
            </td>

            <th scope="row">
                <a class="btn-link btn-deleteDDCTTenFileHinh" data-id="{{Id}}" data-toggle="tooltip"
                            data-placement="top" title="Xóa file hình - video">
                            <i class="fa fa-trash-o"  style="color:red"></i>
                 </a>
            </th>
        </tr>
    </script>
</div>

