<div id="modal-add-edit-XacNhanDVDien" class="bootbox modal fade modal-darkorange in" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" style="overflow-y: auto; max-height:85%;  margin-top: 50px; margin-bottom:50px; ">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="bootbox-close-button close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Xác nhận dịch vụ điện </h4>
            </div>
            <div class="modal-body">

                <form class="form-horizontal form-label-left" id="frmMainXacNhanDVDien">
                    <div class="form-group">
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <label>Ngày xác nhận <a style="color:red">(*)</a></label>
                            <input type="text" name="txtNgayXacNhanDVDien" class="form-control has-feedback-right" id="txtNgayXacNhanDVDien" placeholder="Ngày từ chối dịch vụ điện">
                        </div>
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <label for="lbNhanVienTuChoiGDNDien">Tên nhân viên nhập </label>
                            <input type="text" name="txtNhanVienXacNhanDVDien" class="form-control has-feedback-right" id="txtNhanVienXacNhanDVDien" placeholder="Nhân viên từ chối dịch vụ điện">
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-6 col-sm-12 col-xs-12">
                            <label for="lbGhiChuXacNhanDVDien">Ghi chú từ chối</label>
                            <textarea id="txtGhiChuXacNhanDVDien" required="required" class="form-control" name="txtGhiChuXacNhanDVDien"
                                      data-parsley-trigger="keyup" data-parsley-maxlength="1000">
                            </textarea>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-sm-offset-2 col-sm-10">
                            @*<button type="button" id="btnPhucHoiTuChoiDVDien" class="btn btn-danger">Phục hồi từ chối </button>*@
                            <button type="button" id="btnCancelXacNhanDVDien" data-dismiss="modal" class="btn btn-default">Đóng</button>
                            <button type="button" id="btnSaveXacNhanDVDien" class="btn btn-warning">Xác nhận dịch vụ điện </button>
                        </div>
                    </div>
                </form>

                <div class="ln_solid"></div>
            </div>
        </div>
    </div>
</div>
