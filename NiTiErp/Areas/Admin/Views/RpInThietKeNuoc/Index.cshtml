        thietkenuoc3.Parameters["prTenKHPhai"].Visible = false;
        thietkenuoc3.Parameters["prDanhSoPhai"].Value = @Context.Session.GetString("DanhSoPhai");
        thietkenuoc3.Parameters["prDanhSoPhai"].Visible = false;
        thietkenuoc3.Parameters["prTenKHTrai"].Value = @Context.Session.GetString("TenKHTrai");
        thietkenuoc3.Parameters["prTenKHTrai"].Visible = false;
        thietkenuoc3.Parameters["prDanhSoTrai"].Value = @Context.Session.GetString("DanhSoTrai");
        thietkenuoc3.Parameters["prDanhSoTrai"].Visible = false;

        thietkenuoc3.Parameters["prHinhThietKe1"].Value = @Context.Session.GetString("HinhThietKe1");
        thietkenuoc3.Parameters["prHinhThietKe1"].Visible = false;

    }


    <div class="container-fluid body-content">
        @Html.DevExpress().WebDocumentViewer("DocumentViewer").Height("1000px").Bind(thietkenuoc3)
    </div>




</body>
</html>