    <script src="~/js/viewer.part.bundle.js"></script>
    <script src="~/js/designer.part.bundle.js"></script>

    @{ List<ChiTietThietKeViewModel> chitiettk = SessionHelper.GetObjectFromJson<List<ChiTietThietKeViewModel>>(Context.Session, "listThietKe");

