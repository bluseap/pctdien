        thietkenuoc3.Parameters["prDanhSo"].Visible = false;
        thietkenuoc3.Parameters["prDiaChi"].Value = @Context.Session.GetString("DiaChi");
        thietkenuoc3.Parameters["prDiaChi"].Visible = false;
        thietkenuoc3.Parameters["prDiaChiLapDat"].Value = @Context.Session.GetString("DiaChiLapDat");
        thietkenuoc3.Parameters["prDiaChiLapDat"].Visible = false;
        thietkenuoc3.Parameters["prLyDoThietKe"].Value = @Context.Session.GetString("LyDoThietKe");
